	"scripts": {
		"build": "tsc",
		"startLinux": "NODE_PATH=./src nodemon --exec ts-node src/index.ts",
		"dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/index.ts",
		"genPrisma": "yarn run prisma generate --schema=./src/prisma/schema.prisma --watch",
		"migratePrisma": "yarn run prisma migrate dev --schema=./src/prisma/schema.prisma",
		"prod": "npm run build && npm run start"
	}
